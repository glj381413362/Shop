<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>cn.com.servlet.UserServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>BuyCardServlet</servlet-name>
    <servlet-class>cn.com.servlet.BuyCardServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>CartServlet</servlet-name>
    <servlet-class>cn.com.servlet.CartServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/userServlet.action</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>BuyCardServlet</servlet-name>
    <url-pattern>/buyCardServlet.action</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>CartServlet</servlet-name>
    <url-pattern>/cartServlet.action</url-pattern>
  </servlet-mapping>
  
</web-app>
